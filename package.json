{
  "name": "aisweb-brasil",
  "version": "1.4.0",
  "description": "A lib for retrieving AisWeb API data to JavaScript objects",
  "main": "dist/index.js",
  "repository": "jpedroh/aisweb-brasil",
  "author": "Joao Pedro Henrique",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "lint": "tslint -c tslint.json 'src/**/*.ts'"
  },
  "dependencies": {
    "request": "^2.88.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
