    "@types/node": "^10.5.2",
    "@types/request": "^2.47.1",
    "@types/xml2js": "^0.4.3",
    "jest": "^23.4.1",
    "jest-cli": "^23.4.1",
    "ts-jest": "^23.0.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
